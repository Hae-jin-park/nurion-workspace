{
  "scripts": {
    "startapp" : "python3 manage.py startapp $APP_NAME",
    "server" : "python3 manage.py runserver 0.0.0.0:8000",
    "makemigrate" : "python3 manage.py makemigrations",
    "migrateall" : "python3 manage.py migrate",
    "migrate" : "python3 manage.py migrate $APP_NAME",
    "shellplus" : "python3 manage.py shell_plus",
    "sqlplus" : "python3 manage.py shell_plus --print-sql",
    "diagram" : "python3 manage.py graph_models -a --dot -o model_diagram.dot",
    "pngdot" : "dot -Tpng model_diagram.dot dot -o model_diagram.png",
    "jupyter" : "jupyter notebook"
  }
}
